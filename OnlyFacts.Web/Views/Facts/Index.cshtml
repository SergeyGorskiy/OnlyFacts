@model Calabonga.OperationResults.OperationResult<Calabonga.UnitOfWork.IPagedList<OnlyFacts.Web.ViewModels.FactViewModel>>
@{
    Layout = "_LayoutColumn";
    ViewData["Title"] = "Home Page";
}

@if (!(Model is null))
 {
    foreach (var fact in Model.Result.Items)
    {
        <partial name="_Fact" model="fact" />
    }
 }



